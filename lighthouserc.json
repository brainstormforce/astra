{
  "ci": {
    "collect": {
      "url": [ "http://localhost:8890" ]
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "themed-omnibox": "off",
        "splash-screen": "off",
        "service-worker": "off",
        "meta-description": "off",
        "maskable-icon": "off",
        "installable-manifest": "off",
        "apple-touch-icon": "off"
      }
    }
  }
}
